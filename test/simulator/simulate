#!/bin/sh
clear
node src/simulation.js A $1 &
node src/simulation.js B $1 &
node src/simulation.js C $1 &
node src/simulation.js D $1 &
node src/simulation.js E $1 &

sleep 1s
curl --silent --head http://localhost:3666/dump >/dev/null
wait
curl --silent --head http://localhost:3666/dump >/dev/null

echo ALL done
